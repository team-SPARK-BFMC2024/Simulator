<?xml version="1.0" encoding="UTF-8"?>

<launch>

  <!-- ENTER HIGHWAY SIGNS -->
  <node
      name  = "EHIGH_T"
      pkg   = "gazebo_ros"
      type  = "spawn_model"
      args  = "-file $(find models_pkg)/enter_highway_sign/model.sdf -sdf -model EHIGH_T -x 6.3337 -y 11.5342 -z 0.078998 -R 0  -P 0  -Y 3.141592"/>

  <node
      name  = "EHIGH_Y"
      pkg   = "gazebo_ros"
      type  = "spawn_model"
      args  = "-file $(find models_pkg)/enter_highway_sign/model.sdf -sdf -model EHIGH_Y -x 14.3279 -y 11.04 -z 0.078998 -R 0  -P 0  -Y -0.029883"/>

</launch>
